<odoo>
    <data>
        <!--Template Material Grey Light Green-->
        <record model="theme_kit.top_panel" id="material_grey_light_green_top_panel">
            <field name="name">Material Grey Light Green</field>
            <field name="top_panel_bg">#F5F5F5</field>
            <field name="top_panel_bg_active">True</field>
            <field name="top_panel_border">#BDBDBD</field>
            <field name="top_panel_border_active">True</field>
            <field name="top_panel_font">#212121</field>
            <field name="top_panel_font_active">True</field>
            <field name="top_panel_active_item_font">#FFFFFF</field>
            <field name="top_panel_active_item_font_active">True</field>
            <field name="top_panel_active_item_bg">#616161</field>
            <field name="top_panel_active_item_bg_active">True</field>
            <field name="top_panel_hover_item_font">#FFFFFF</field>
            <field name="top_panel_hover_item_font_active">True</field>
            <field name="top_panel_hover_item_bg">#9E9E9E</field>
            <field name="top_panel_hover_item_bg_active">True</field>
        </record>

        <record model="theme_kit.left_panel" id="material_grey_light_green_left_panel">
            <field name="name">Material Grey Light Green</field>
            <field name="left_panel_bg">#F5F5F5</field>
            <field name="left_panel_bg_active">True</field>
            <field name="left_panel_main_menu">#212121</field>
            <field name="left_panel_main_menu_active">True</field>
            <field name="left_panel_sub_menu">#757575</field>
            <field name="left_panel_sub_menu_active">True</field>
            <field name="left_panel_active_item_font">#FFFFFF</field>
            <field name="left_panel_active_item_font_active">True</field>
            <field name="left_panel_active_item_bg">#616161</field>
            <field name="left_panel_active_item_bg_active">True</field>
            <field name="left_panel_hover_item_font">#FFFFFF</field>
            <field name="left_panel_hover_item_font_active">True</field>
            <field name="left_panel_hover_item_bg">#9E9E9E</field>
            <field name="left_panel_hover_item_bg_active">True</field>
        </record>

        <record model="theme_kit.content" id="material_grey_light_green_content">
            <field name="name">Material Grey Light Green</field>
            <field name="content_bg">#FDFDFD</field>
            <field name="content_bg_active">True</field>
            <field name="content_button">#8BC34A</field>
            <field name="content_button_active">True</field>
            <field name="content_form">#FFFFFF</field>
            <field name="content_form_active">True</field>
            <field name="content_form_text">#212121</field>
            <field name="content_form_text_active">True</field>
            <field name="content_form_title">#212121</field>
            <field name="content_form_title_active">True</field>
            <field name="content_text">#212121</field>
            <field name="content_text_active">True</field>
            <field name="content_form_link">#212121</field>
            <field name="content_form_link_active">True</field>
            <field name="content_loader">#8BC34A</field>
            <field name="content_loader_active">True</field>
            <field name="content_loader_text">#FFFFFF</field>
            <field name="content_loader_text_active">True</field>
        </record>

        <record model="theme_kit.theme" id="material_grey_light_green_theme">
            <field name="name">Material Grey Light Green</field>
            <field name="top_panel_id" ref="material_grey_light_green_top_panel"/>
            <field name="left_panel_id" ref="material_grey_light_green_left_panel"/>
            <field name="content_id" ref="material_grey_light_green_content"/>
        </record>
    </data>
</odoo>
